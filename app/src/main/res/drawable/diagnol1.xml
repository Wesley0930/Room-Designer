<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="-140"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="15dp"
                    android:color="#E91E63"/>
            </shape>
        </rotate>
    </item>
</selector>